{
  "name": "@vnode/errors",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@vnode/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/errors/src",
    "tags": [
      "lib"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/errors/dist",
          "main": "libs/errors/src/index.ts",
          "tsConfig": "libs/errors/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        },
        "dependsOn": [
          "prebuild"
        ]
      },
      "prebuild": {
        "executor": "nx:run-commands",
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/src/lib/errors.ts"
        ],
        "options": {
          "commands": [
            "node ./scripts/build.mjs"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "vitest": "^4.0.0",
    "@vnode/types": "workspace:*"
  }
}